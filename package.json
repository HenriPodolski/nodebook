{
  "name": "nodebook",
  "version": "0.0.0",
  "description": "A rapid data science and prototyping tool",
  "scripts": {
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "lint:ts": "tslint -c tslint.json -p tsconfig.json",
    "build:scss": "node-sass src -o dist",
    "watch:scss": "node-sass -w src -o dist",
    "build:css": "tcm src -o dist",
    "watch:css": "tcm -w src -o dist",
    "watch:electron": "cross-env NODE_ENV=development UPGRADE_EXTENSIONS=1 electron ./dist/main.js",
    "build": "concurrently --kill-others \"npm run build:ts\" \"npm run build:scss\" \"npm run build:css\"",
    "watch": "concurrently --kill-others \"npm run watch:ts\" \"npm run watch:scss\" \"npm run watch:electron\" \"npm run watch:css\"",
    "lint": "",
    "prod": "",
    "dev": "concurrently --kill-others \"npm run watch\"",
    "start": "npm run dev"
  },
  "repository": "",
  "keywords": [
    "data"
  ],
  "author": "Henri Podolski",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.0.3",
    "@types/history": "^4.6.2",
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.6",
    "concurrently": "^3.6.0",
    "cross-env": "^5.2.0",
    "electron": "^2.0.2",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.4",
    "node-sass": "^4.9.2",
    "redux-logger": "^3.0.6",
    "tslint": "^5.10.0",
    "typed-css-modules": "^0.3.4",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "brace": "^0.11.1",
    "history": "^4.7.2",
    "react": "^16.4.1",
    "react-ace": "^6.1.1",
    "react-dom": "^16.4.1",
    "react-frame-component": "^4.0.0",
    "react-markdown": "^3.3.4",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-observable": "^1.0.0",
    "rxjs": "^6.2.1"
  }
}
